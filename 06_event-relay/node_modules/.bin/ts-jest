#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/valery/Projects/platform-ecosystem/node_modules/.pnpm/ts-jest@29.4.6_@babel+core@7.28.6_jest@30.2.0_typescript@5.9.3/node_modules/ts-jest/node_modules:/Users/valery/Projects/platform-ecosystem/node_modules/.pnpm/ts-jest@29.4.6_@babel+core@7.28.6_jest@30.2.0_typescript@5.9.3/node_modules:/Users/valery/Projects/platform-ecosystem/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/valery/Projects/platform-ecosystem/node_modules/.pnpm/ts-jest@29.4.6_@babel+core@7.28.6_jest@30.2.0_typescript@5.9.3/node_modules/ts-jest/node_modules:/Users/valery/Projects/platform-ecosystem/node_modules/.pnpm/ts-jest@29.4.6_@babel+core@7.28.6_jest@30.2.0_typescript@5.9.3/node_modules:/Users/valery/Projects/platform-ecosystem/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
